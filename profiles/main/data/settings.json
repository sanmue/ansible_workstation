{
  "editor.minimap.enabled": false,
  "editor.fontFamily": "'MesloLGS NF','Droid Sans Mono', 'monospace', monospace",
  "editor.fontSize": 18,
  "telemetry.telemetryLevel": "crash",
  "terminal.integrated.shellIntegration.history": 200,
  "terminal.integrated.defaultProfile.linux": "zsh",
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "files.trimTrailingWhitespace": true,
  "prettier.tabWidth": 4,
  "[ansible]": {
    "editor.defaultFormatter": "kennylong.kubernetes-yaml-formatter",
    "editor.formatOnSave": false,
    "editor.detectIndentation": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.quickSuggestions": {
      "comments": true,
      "other": true,
      "strings": true
    },
    "editor.autoIndent": "advanced"
  },
  "ansible.python.interpreterPath": "/home/sandro/.pyenv/versions/3.12.7/bin/python",
  "files.autoSave": "onFocusChange",
  "terminal.integrated.scrollback": 10000,
  "git.statusLimit": 16384
}
